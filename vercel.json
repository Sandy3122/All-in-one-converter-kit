{
  "version": 2,
  "builds": [
    {
      "src": "./server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/imgToPdf/(.*)",
      "dest": "/public/imgToPdf/$1"
    },
    {
      "src": ".*",
      "dest": "./server.js"
    }
  ]
}
