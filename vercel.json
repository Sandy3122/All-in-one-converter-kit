{
  "builds": [
      {
          "src": "server.js",
          "use": "@vercel/node"
      }
  ],
  "routes": [
      {
          "src": "/public/imgToPdf/convert",
          "dest": "/server.js"
      },
      {
          "src": "/public/imgToPdf/download/(.*)",
          "dest": "/server.js"
      }
  ]
}
