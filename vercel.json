{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/public/imgToPdf/convert",
      "dest": "/server.js"
  },
  {
      "src": "/public/imgToPdf/download/(.*)",
      "dest": "/server.js"
  }
  ]
}
